!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
COMMON_FOO	foo.php	/^define('COMMON_FOO', 42);$/;"	d
CommonFoo	foo.php	/^class CommonFoo {$/;"	c
CommonTrait	foo.php	/^trait CommonTrait {}$/;"	t
common_plain_old_function	foo.php	/^function common_plain_old_function(){}$/;"	f
common_plain_old_function_with_arguments	foo.php	/^function common_plain_old_function_with_arguments($a, $b=''){}$/;"	f
common_private_method	foo.php	/^    private function common_private_method($foo) { }$/;"	f
common_private_static_method	foo.php	/^    private static function common_private_static_method($foo) { }$/;"	f
common_protected_method	foo.php	/^    protected function common_protected_method($foo) { }$/;"	f
common_protected_static_method	foo.php	/^    protected static function common_protected_static_method($foo) { }$/;"	f
common_public_method	foo.php	/^    public function common_public_method($foo) { }$/;"	f
common_public_static_method	foo.php	/^    public static function common_public_static_method($foo) { }$/;"	f
common_static_public_method	foo.php	/^    static public function common_static_public_method($foo) { }$/;"	f
